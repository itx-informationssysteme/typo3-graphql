{
    "name": "itx/typo3-graphql",
    "type": "typo3-cms-extension",
    "description": "This extension provides a GraphQL API for TYPO3 CMS",
	"license": "GPL-3.0-or-later",
	"homepage": "http://www.itx.de",
    "authors": [
        {
            "name": "Benjamin Jasper",
            "role": "Developer"
        }
    ],
    "require": {
		"php": ">=8.2",
        "typo3/cms-core": "^12.4",
		"typo3/cms-extbase": "^12.4",
		"typo3/cms-frontend": "^12.4",
		"webonyx/graphql-php": "15.*",
		"simpod/graphql-utils": "^0.7",
		"aimeos/map": "^3"
    },
	"require-dev": {
		"phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Itx\\Typo3GraphQL\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itx\\Typo3GraphQL\\Tests\\": "Tests"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "typo3_graphql"
        }
    }
}
